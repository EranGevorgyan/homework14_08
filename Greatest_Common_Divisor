number1 = int(input("number1: "))
number2 = int(input("number2: "))
ls = []

for i in range(1, min(number1, number2) + 1):
    if number2 % i == 0 and number1 % i == 0:
        ls.append(i)

max = ls[0]
for i in ls:
    if i > max:
        max = i

print("The greatest common divisor (GCD) is:", max)
